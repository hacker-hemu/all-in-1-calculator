            <script>

                function openCalculator(evt, calculatorName) {
                    var i, tabContent, tabLinks;
                    tabContent = document.getElementsByClassName("calculator-content");
                    for (i = 0; i < tabContent.length; i++) {
                        tabContent[i].style.display = "none";
                    }
                    tabLinks = document.getElementsByClassName("tab-link");
                    for (i = 0; i < tabLinks.length; i++) {
                        tabLinks[i].classList.remove("active");
                    }
                    document.getElementById(calculatorName).style.display = "block";
                    evt.currentTarget.classList.add("active");
                }

                // Complex Number Calculator Logic
                function complexNumberOperations(operation) {
                    const real1 = parseFloat(document.getElementById("real1").value);
                    const imag1 = parseFloat(document.getElementById("imag1").value);
                    const real2 = parseFloat(document.getElementById("real2").value);
                    const imag2 = parseFloat(document.getElementById("imag2").value);

                    if (isNaN(real1) || isNaN(imag1) || isNaN(real2) || isNaN(imag2)) {
                        alert("Please enter valid numbers");
                        return;
                    }

                    let resultReal, resultImag;
                    if (operation === 'add') {
                        resultReal = real1 + real2;
                        resultImag = imag1 + imag2;
                    } else if (operation === 'subtract') {
                        resultReal = real1 - real2;
                        resultImag = imag1 - imag2;
                    } else if (operation === 'multiply') {
                        resultReal = real1 * real2 - imag1 * imag2;
                        resultImag = real1 * imag2 + imag1 * real2;
                    }

                    document.getElementById("complexResult").innerHTML = `Result: ${resultReal} + ${resultImag}i`;
                }

                // Boolean Algebra Simplifier Logic
                function simplifyBooleanExpression() {
                    const expression = document.getElementById("booleanExpression").value.trim();
                    const resultElement = document.getElementById("booleanResult");

                    if (expression === "") {
                        resultElement.innerText = "Please enter a Boolean expression.";
                        return;
                    }

                    try {
                        // Step 1: Parse the input expression
                        const parsedExpression = parseExpression(expression);

                        // Step 2: Simplify using Boolean laws
                        const simplifiedExpression = simplifyUsingLaws(parsedExpression);

                        // Step 3: Validate and display the result
                        resultElement.innerText = `Simplified Boolean Expression: ${simplifiedExpression}`;
                    } catch (error) {
                        resultElement.innerText = "Invalid Boolean expression.";
                        console.error(error);
                    }
                }

                // Parse the input expression
                function parseExpression(expression) {
                    // Replace logical operators for consistency
                    return expression
                        .replace(/\s+/g, "") // Remove spaces
                        .replace(/AND/gi, "&")
                        .replace(/OR/gi, "|")
                        .replace(/NOT/gi, "!");
                }

                // Simplify using Boolean laws
                function simplifyUsingLaws(expression) {
                    let result = expression;

                    // Step 1: Apply De Morgan's Laws
                    // !(A & B) -> !A | !B
                    result = result.replace(/!\((\w)&(\w)\)/g, "!$1|!$2");
                    // !(A | B) -> !A & !B
                    result = result.replace(/!\((\w)\|(\w)\)/g, "!$1&!$2");

                    // Step 2: Apply Redundancy Elimination
                    // A | A = A, A & A = A
                    result = result.replace(/(\w)\|(\1)/g, "$1");
                    result = result.replace(/(\w)&(\1)/g, "$1");

                    // Step 3: Simplify Double Negations
                    // !!A = A
                    result = result.replace(/!!(\w)/g, "$1");

                    // Step 4: Apply Constant Elimination
                    // A & 1 = A, A | 0 = A
                    result = result.replace(/(\w)&1/g, "$1");
                    result = result.replace(/(\w)\|0/g, "$1");
                    // A & 0 = 0, A | 1 = 1
                    result = result.replace(/(\w)&0/g, "0");
                    result = result.replace(/(\w)\|1/g, "1");

                    // Step 5: Apply Absorption Laws
                    // A | (A & B) = A
                    result = result.replace(/(\w)\|(\1&\w)/g, "$1");
                    // A & (A | B) = A
                    result = result.replace(/(\w)&(\1\|\w)/g, "$1");

                    // Step 6: Apply Distributive Laws
                    // A & (B | C) = (A & B) | (A & C)
                    result = result.replace(/(\w)&\((\w)\|(\w)\)/g, "($1&$2)|($1&$3)");
                    // A | (B & C) = (A | B) & (A | C)
                    result = result.replace(/(\w)\|\((\w)&(\w)\)/g, "($1|$2)&($1|$3)");

                    // Step 7: Apply Complement Laws
                    // A | !A = 1, A & !A = 0
                    result = result.replace(/(\w)\|\!\1/g, "1");
                    result = result.replace(/(\w)&\!\1/g, "0");

                    // Step 8: Apply Idempotent Laws
                    // A | A = A, A & A = A
                    result = result.replace(/(\w)\|\1/g, "$1");
                    result = result.replace(/(\w)&\1/g, "$1");

                    // Step 9: Apply Consensus Theorem
                    // (A & B) | (!A & C) = (B | C)
                    result = result.replace(/\((\w)&(\w)\)\|\(!\1&(\w)\)/g, "($2|$3)");

                    // Step 10: Apply Distributive Expansion
                    // A & (B & C) = (A & B) & C
                    result = result.replace(/(\w)&\((\w)&(\w)\)/g, "($1&$2)&$3");
                    // A | (B | C) = (A | B) | C
                    result = result.replace(/(\w)\|\((\w)\|(\w)\)/g, "($1|$2)|$3");

                    // Step 11: XOR Simplification
                    // A ^ B = (A & !B) | (!A & B)
                    result = result.replace(/(\w)\^(\w)/g, "($1&!$2)|(!$1&$2)");

                    // Step 12: Apply Absorption Variants
                    // (A & B) | (A & !B) = A
                    result = result.replace(/\((\w)&(\w)\)\|\(\1&!\2\)/g, "$1");
                    // (A | B) & (A | !B) = A
                    result = result.replace(/\((\w)\|(\w)\)&\(\1\|!\2\)/g, "$1");

                    // Step 13: Custom Rules (Example for NOR and NAND)
                    // A NOR B = !(A | B)
                    result = result.replace(/(\w)~\|(\w)/g, "!($1|$2)");
                    // A NAND B = !(A & B)
                    result = result.replace(/(\w)~&(\w)/g, "!($1&$2)");

                    return result;
                }

                function iterativeSimplification(expression) {
                    let prev = "";
                    let current = expression;
                    while (current !== prev) {
                        prev = current;
                        current = simplifyUsingLaws(current);
                    }
                    return current;
                }





                // Partial Fraction Decomposer Logic
                function decomposePartialFraction() {
                    const rationalFunction = document.getElementById("rationalFunction").value.trim();
                    const resultElement = document.getElementById("partialResult");

                    // Clear previous result
                    resultElement.innerHTML = "";

                    if (!rationalFunction) {
                        resultElement.innerText = "Please enter a rational function (e.g., 1/(x^2 - 4)).";
                        return;
                    }

                    // Extract numerator and denominator using regex
                    const regex = /^([0-9]+)\s*\/\s*\((.+)\)$/;
                    const match = rationalFunction.match(regex);

                    if (!match) {
                        resultElement.innerText = "Invalid input. Use the format: numerator/(denominator).";
                        return;
                    }

                    const numerator = parseFloat(match[1]);
                    const denominatorExpr = match[2].trim();

                    // Handle specific cases for partial fraction decomposition

                    if (denominatorExpr === "x^2 - 4") {
                        // Case 1: x^2 - 4 = (x - 2)(x + 2)
                        const A = numerator / 2;
                        const B = -numerator / 2;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/(x^2 - 4) = ${A}/(x - 2) + ${B}/(x + 2)</p>`;
                    } else if (denominatorExpr === "(x - 1)^2") {
                        // Case 2: (x - 1)^2 (repeated root)
                        const A = numerator;
                        const B = 0;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/(x - 1)^2 = ${A}/(x - 1)^2 + ${B}/(x - 1)</p>`;
                    } else if (denominatorExpr === "(x - 1)^3") {
                        // Case 3: Repeated root (x - 1)^3
                        const A = numerator;
                        const B = 0;
                        const C = 0;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/(x - 1)^3 = ${A}/(x - 1)^3 + ${B}/(x - 1)^2 + ${C}/(x - 1)</p>`;
                    } else if (denominatorExpr === "x^2 + 3x + 2") {
                        // Case 4: Irreducible quadratic x^2 + 3x + 2
                        const A = numerator / 3;
                        const B = -numerator / 2;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/(x^2 + 3x + 2) = ${A}/(x + 1) + ${B}/(x + 2)</p>`;
                    } else if (denominatorExpr === "x^3 - x") {
                        // Case 5: Cubic factorization x(x - 1)(x + 1)
                        const A = numerator / 3;
                        const B = 0;
                        const C = 0;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/(x^3 - x) = ${A}/x + ${B}/(x - 1) + ${C}/(x + 1)</p>`;
                    } else if (denominatorExpr === "x^2 - 1") {
                        // Case 6: x^2 - 1 = (x - 1)(x + 1)
                        const A = numerator / 2;
                        const B = -numerator / 2;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/(x^2 - 1) = ${A}/(x - 1) + ${B}/(x + 1)</p>`;
                    } else if (denominatorExpr === "(x - 2)(x + 3)") {
                        // Case 7: Linear factors (x - 2)(x + 3)
                        const A = numerator / 5;  // Simplified example for demonstration
                        const B = -numerator / 5;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/((x - 2)(x + 3)) = ${A}/(x - 2) + ${B}/(x + 3)</p>`;
                    } else if (denominatorExpr === "(x - 1)(x + 1)") {
                        // Case 8: Linear factors (x - 1)(x + 1)
                        const A = numerator / 2;
                        const B = -numerator / 2;
                        resultElement.innerHTML = `<p>Decomposed: ${numerator}/((x - 1)(x + 1)) = ${A}/(x - 1) + ${B}/(x + 1)</p>`;
                    } else {
                        resultElement.innerHTML = `<p>Decomposition not implemented for: ${denominatorExpr}</p>`;
                    }
                }



                // Inequality Solver Logic
                function solveInequality() {
                    const inequalityInput = document.getElementById("inequalityExpression").value.trim();
                    const resultElement = document.getElementById("inequalityResult");

                    // Clear previous result
                    resultElement.innerText = "";

                    // Check if input is empty
                    if (inequalityInput === "") {
                        resultElement.innerText = "Please enter an inequality.";
                        return;
                    }

                    // Handle Linear Inequalities (e.g., 2*x + 3 > 0, 4*x - 5 < 0)
                    const linearRegex = /^(-?\d+)\s*\*?x\s*([+-]?\s*\d+)\s*([<>]=?)\s*0$/;
                    const matchLinear = inequalityInput.match(linearRegex);

                    if (matchLinear) {
                        const a = parseFloat(matchLinear[1]);
                        const b = parseFloat(matchLinear[2].replace(/\s+/g, '')); // Clean spaces from b term
                        const operator = matchLinear[3].trim();

                        let solution;

                        // Solve for x in linear inequality
                        if (operator === ">") {
                            solution = `x > ${-b / a}`;
                        } else if (operator === "<") {
                            solution = `x < ${-b / a}`;
                        } else if (operator === ">=") {
                            solution = `x ≥ ${-b / a}`;
                        } else if (operator === "<=") {
                            solution = `x ≤ ${-b / a}`;
                        }

                        resultElement.innerText = `Solution: ${solution}`;
                        return;
                    }

                    // Handle Quadratic Inequalities (e.g., x^2 - 4 > 0, 2*x^2 + 3*x - 2 < 0)
                    const quadraticRegex = /^(-?\d+)\s*\*?x\^2\s*([+-]?\s*\d+)\s*\*?x\s*([+-]?\s*\d+)\s*([<>]=?)\s*0$/;
                    const matchQuadratic = inequalityInput.match(quadraticRegex);

                    if (matchQuadratic) {
                        const a = parseFloat(matchQuadratic[1]);
                        const b = parseFloat(matchQuadratic[2].replace(/\s+/g, '')) || 0;
                        const c = parseFloat(matchQuadratic[3].replace(/\s+/g, '')) || 0;
                        const operator = matchQuadratic[4].trim();

                        // Calculate discriminant (Delta = b^2 - 4ac)
                        const discriminant = b * b - 4 * a * c;
                        let solution;

                        // If discriminant < 0: no real roots, check the sign of a
                        if (discriminant < 0) {
                            if (a > 0 && operator === ">") {
                                solution = `Solution: x ∈ (-∞, ∞)`;
                            } else if (a < 0 && operator === "<") {
                                solution = `Solution: x ∈ (-∞, ∞)`;
                            } else {
                                solution = "Solution: No solution.";
                            }
                        }
                        // If discriminant == 0: one real root
                        else if (discriminant === 0) {
                            const root = -b / (2 * a);
                            if (operator === ">") {
                                solution = `Solution: x > ${root}`;
                            } else if (operator === "<") {
                                solution = `Solution: x < ${root}`;
                            } else if (operator === ">=") {
                                solution = `Solution: x ≥ ${root}`;
                            } else if (operator === "<=") {
                                solution = `Solution: x ≤ ${root}`;
                            }
                        }
                        // If discriminant > 0: two real roots
                        else {
                            const sqrtDisc = Math.sqrt(discriminant);
                            const root1 = (-b + sqrtDisc) / (2 * a);
                            const root2 = (-b - sqrtDisc) / (2 * a);

                            if (a > 0) {
                                if (operator === ">") {
                                    solution = `Solution: x < ${root2} or x > ${root1}`;
                                } else if (operator === "<") {
                                    solution = `Solution: ${root2} < x < ${root1}`;
                                } else if (operator === ">=") {
                                    solution = `Solution: x ≤ ${root2} or x ≥ ${root1}`;
                                } else if (operator === "<=") {
                                    solution = `Solution: ${root2} ≤ x ≤ ${root1}`;
                                }
                            } else {
                                if (operator === ">") {
                                    solution = `Solution: ${root2} < x < ${root1}`;
                                } else if (operator === "<") {
                                    solution = `Solution: x < ${root2} or x > ${root1}`;
                                } else if (operator === ">=") {
                                    solution = `Solution: x ≥ ${root2} or x ≤ ${root1}`;
                                } else if (operator === "<=") {
                                    solution = `Solution: ${root2} ≤ x ≤ ${root1}`;
                                }
                            }
                        }

                        resultElement.innerText = solution;
                        return;
                    }

                    // If the input doesn't match linear or quadratic formats
                    resultElement.innerText = "Inequality format not recognized. Please follow the format.";
                }

                // Flow Rate Calculator Logic
                function calculateFlowRate() {
                    const diameter = parseFloat(document.getElementById("pipeDiameter").value);
                    const velocity = parseFloat(document.getElementById("velocity").value);
                    const flowRate = Math.PI * Math.pow(diameter / 2, 2) * velocity;
                    document.getElementById("flowRateResult").innerHTML = `Flow Rate: ${flowRate.toFixed(2)} cubic meters per second`;
                }

                // Impulse Calculator Logic
                function calculateImpulse() {
                    const force = parseFloat(document.getElementById("force").value);
                    const time = parseFloat(document.getElementById("time").value);
                    const impulse = force * time;
                    document.getElementById("impulseResult").innerHTML = `Impulse: ${impulse.toFixed(2)} Ns`;
                }

                // Factorization Calculator Logic
                function calculateFactorization() {
                    const number = parseInt(document.getElementById("factorizeNumber").value);
                    let factors = [];
                    for (let i = 2; i <= number; i++) {
                        while (number % i === 0) {
                            factors.push(i);
                            number /= i;
                        }
                    }
                    document.getElementById("factorizationResult").innerHTML = `Prime Factors: ${factors.join(' x ')}`;
                }

                // Percentage Calculator Logic
                function calculatePercentage() {
                    const originalValue = parseFloat(document.getElementById("originalValue").value);
                    const percentageChange = parseFloat(document.getElementById("percentageChange").value);
                    const newValue = originalValue * (1 + percentageChange / 100);
                    document.getElementById("percentageResult").innerHTML = `New Value: ${newValue.toFixed(2)}`;
                }

                // Exponential Calculator Logic
                function calculateExponential() {
                    const base = parseFloat(document.getElementById("base").value);
                    const exponent = parseFloat(document.getElementById("exponent").value);
                    const result = Math.pow(base, exponent);
                    document.getElementById("exponentialResult").innerHTML = `Result: ${result.toFixed(2)}`;
                }

                // Cube Calculator Logic
                function calculateCube() {
                    const number = parseFloat(document.getElementById("cubeNumber").value);
                    const cube = Math.pow(number, 3);
                    document.getElementById("cubeResult").innerHTML = `Cube: ${cube.toFixed(2)}`;
                }

                // Cube Root Calculator Logic
                function calculateCubeRoot() {
                    const number = parseFloat(document.getElementById("cubeRootNumber").value);
                    const cubeRoot = Math.cbrt(number);
                    document.getElementById("cubeRootResult").innerHTML = `Cube Root: ${cubeRoot.toFixed(2)}`;
                }
